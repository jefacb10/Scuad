@model Scuad.ViewModel.UsersViewModel
    <table class="table" width="100px">
        <thead class="thead-dark">
            <tr>
                <th scope="col">&nbsp;</th>
                <th scope="col">@Html.DisplayName("Nome")</th>
                <th scope="col">@Html.DisplayName("Cargo")</th>
                <th scope="col">@Html.DisplayName("Ativo")</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var usuario in Model.UsersList)
            {
                <tr>
                    <th>&nbsp;</th>
                    <td>@Html.DisplayFor(model => usuario.UserName)</td>
                    <td>@Html.DisplayFor(model => usuario.Charges.Name)</td>
                    <td>@Html.DisplayFor(model => usuario.IsActive)</td>
                </tr>
            }
        </tbody>
    </table>